<div class="container">
  <h2>Edit memory</h2>
  <%= simple_form_for @geomem do |f| %>
    <%= f.input :title %>
    <%= f.input :description%>
    <%= f.input :location%>
    <div id="location-results" class="list-group"></div>
    <%= f.hidden_field :coordinates%>
    <%= f.submit "Edit memory", class: "btn btn-primary" %>
  <%end%>
</div>
<%= javascript_pack_tag 'edit_autocomplete' %>